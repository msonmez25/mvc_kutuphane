@model MvcKutuphane.Models.Entity.Tbl_Uyeler

@using (Html.BeginForm("Index2", "Panelim", FormMethod.Post))
{
<form class="form-horizontal">
    <div class="form-group row">
        <label for="inputName" class="col-sm-2 col-form-label">Ad</label>
        <div class="col-sm-10">
            @Html.TextBoxFor(x => x.AD, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group row">
        <label for="inputName" class="col-sm-2 col-form-label">Soyad</label>
        <div class="col-sm-10">
            @Html.TextBoxFor(x => x.SOYAD, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group row">
        <label for="inputName" class="col-sm-2 col-form-label">Okul</label>
        <div class="col-sm-10">
            @Html.TextBoxFor(x => x.EGITIM, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group row">
        <label for="inputName" class="col-sm-2 col-form-label">Fotograf</label>
        <div class="col-sm-10">
            @Html.TextBoxFor(x => x.FOROGRAF, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group row">
        <label for="inputName" class="col-sm-2 col-form-label">Mail Adresi</label>
        <div class="col-sm-10">
            @Html.TextBoxFor(x => x.MAIL, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group row">
        <label for="inputName" class="col-sm-2 col-form-label">Kullanıcı</label>
        <div class="col-sm-10">
            @Html.TextBoxFor(x => x.KULLANICIADI, new { @class = "form-control" })
        </div>
    </div><div class="form-group row">
        <label for="inputName" class="col-sm-2 col-form-label">Şifre</label>
        <div class="col-sm-10">
            @Html.TextBoxFor(x => x.SIFRE, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group row">
        <div class="offset-sm-2 col-sm-10">
            <div class="checkbox">
                <label>
                    <input type="checkbox"> Şartları okudum <a href="#">kabul ediyorum.</a>
                </label>
            </div>
        </div>
    </div>
    <div class="form-group row">
        <div class="offset-sm-2 col-sm-10">
            <button type="submit" class="btn btn-success">Bilgilerimi Güncelle</button>
        </div>
    </div>

</form>
}